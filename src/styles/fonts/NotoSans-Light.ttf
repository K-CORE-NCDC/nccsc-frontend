<script>

function localeChange(element) {
	let path = '/error/404';	
	
	if(element.value == 'en') {
		if(!path.startsWith("/en/")) {
			path = "/en" + path;
		}
		
		path += "?lang=en";
	} else {
		if(path.startsWith("/en/")) {
			path = path.substring(3);
		}
		path += "?lang=ko"
	}
	location.href=path;
}
function localeChangeMobile(element) {
	let path = '/error/404';	
	
	if(element == 'en') {
		if(!path.startsWith("/en/")) {
			path = "/en" + path;
		}
		
		path += "?lang=en";
	} else {
		if(path.startsWith("/en/")) {
			path = path.substring(3);
		}
		path += "?lang=ko"
	}
	location.href=path;
}
